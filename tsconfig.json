{
  "compilerOptions": {
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "noUnusedParameters": true,
    "rootDir": "src",
    "baseUrl": "src",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "sourceMap": false
  }
}
